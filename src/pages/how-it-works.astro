<script>
  // Horizontal card reveal on vertical scroll with Intersection Observer
  (function() {
    function initHorizontalCardReveal() {
      const cards = document.querySelectorAll('.work-step');
      
      if (cards.length === 0) return;
      
      // Intersection Observer options
      const observerOptions = {
        root: null, // viewport
        rootMargin: '-10% 0px -20% 0px', // Trigger when card is 10% into viewport
        threshold: [0, 0.1, 0.3, 0.5] // Multiple thresholds for smooth detection
      };
      
      // Create observer
      const observer = new IntersectionObserver((entries) => {
        entries.forEach((entry) => {
          const card = entry.target;
          
          // Add stagger delay based on card index
          const cardIndex = parseInt(card.dataset.step) - 1;
          const delay = cardIndex * 150; // 150ms stagger between cards
          
          if (entry.isIntersecting && entry.intersectionRatio > 0.1) {
            // Card is visible - trigger slide in animation
            setTimeout(() => {
              card.classList.add('is-visible');
            }, delay);
          } else if (entry.boundingClientRect.top > window.innerHeight) {
            // Card is below viewport - reset for re-animation
            card.classList.remove('is-visible');
          }
        });
      }, observerOptions);
      
      // Observe all cards
      cards.forEach((card) => {
        observer.observe(card);
      });
      
      // Cleanup on page unload
      window.addEventListener('beforeunload', () => {
        observer.disconnect();
      });
    }
    
    // Initialize reveal animations for this page
    function initRevealAnimations() {
      try {
        if (window.revealInstance) {
          return;
        }
        
        if (typeof window.initReveal === 'function') {
          window.initReveal();
        } else {
          import('/src/lib/reveal-init.js').then(() => {
            if (typeof window.initReveal === 'function') {
              window.initReveal();
            }
          }).catch(() => {
            // Silently fail - content is already visible
          });
        }
      } catch (e) {
        // Silently fail - content is already visible
      }
    }
    
    // Initialize
    if (document.readyState === 'loading') {
      document.addEventListener('DOMContentLoaded', () => {
        initRevealAnimations();
        initHorizontalCardReveal();
      });
    } else {
      initRevealAnimations();
      initHorizontalCardReveal();
    }
  })();
  
  // FAQ Accordion - Smooth animation without bugs
  (function() {
    function initFAQ() {
      const faqItems = document.querySelectorAll('.faq-item');
      
      if (faqItems.length === 0) return;
      
      faqItems.forEach(item => {
        const question = item.querySelector('.faq-question');
        const answer = item.querySelector('.faq-answer');
        
        if (!question || !answer) return;
        
        // Set initial state
        if (item.getAttribute('aria-expanded') !== 'true') {
          answer.style.maxHeight = '0px';
          answer.style.opacity = '0';
        }
        
        question.addEventListener('click', function(e) {
          e.preventDefault();
          
          const isExpanded = item.getAttribute('aria-expanded') === 'true';
          
          // Close all other items
          faqItems.forEach(otherItem => {
            if (otherItem !== item) {
              const otherQuestion = otherItem.querySelector('.faq-question');
              const otherAnswer = otherItem.querySelector('.faq-answer');
              
              if (otherAnswer) {
                otherAnswer.style.maxHeight = '0px';
                otherAnswer.style.opacity = '0';
              }
              
              otherItem.setAttribute('aria-expanded', 'false');
              if (otherQuestion) {
                otherQuestion.setAttribute('aria-expanded', 'false');
              }
            }
          });
          
          // Toggle current item
          if (isExpanded) {
            // Close
            answer.style.maxHeight = '0px';
            answer.style.opacity = '0';
            item.setAttribute('aria-expanded', 'false');
            question.setAttribute('aria-expanded', 'false');
          } else {
            // Open
            answer.style.maxHeight = answer.scrollHeight + 'px';
            answer.style.opacity = '1';
            item.setAttribute('aria-expanded', 'true');
            question.setAttribute('aria-expanded', 'true');
          }
        });
        
        // Handle dynamic content changes
        const resizeObserver = new ResizeObserver(() => {
          if (item.getAttribute('aria-expanded') === 'true') {
            answer.style.maxHeight = answer.scrollHeight + 'px';
          }
        });
        
        resizeObserver.observe(answer);
      });
    }
    
    if (document.readyState === 'loading') {
      document.addEventListener('DOMContentLoaded', initFAQ);
    } else {
      initFAQ();
    }
  })();
</script>

