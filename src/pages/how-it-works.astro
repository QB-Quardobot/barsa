  .testimonials-swiper .swiper-slide {
    height: auto;
    display: flex;
    align-items: stretch;
    flex-shrink: 0;
    
    /* CRITICAL: Scroll snap alignment for CSS mode */
    scroll-snap-align: center;
    scroll-snap-stop: always;
    
    /* Full width on mobile */
