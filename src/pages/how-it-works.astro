  /* ============================================
     SHIMMER & LAZY LOADING STYLES
     Modern placeholder animations for images
     ============================================ */
  
  .image-wrapper {
    position: relative;
    overflow: hidden;
    background-color: rgba(15, 15, 20, 0.6);
  }
  
  .shimmer-wrapper {
    position: relative;
    width: 100%;
    height: 100%;
    overflow: hidden;
  }
  
  .shimmer-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(
      90deg,
      transparent 0%,
      rgba(96, 165, 250, 0.08) 20%,
      rgba(96, 165, 250, 0.15) 50%,
      rgba(96, 165, 250, 0.08) 80%,
      transparent 100%
    );
    animation: shimmer 2s infinite;
    pointer-events: none;
    z-index: 1;
  }
  
  @keyframes shimmer {
    0% {
      transform: translateX(-100%);
    }
    100% {
      transform: translateX(100%);
    }
  }
  
  /* Hide shimmer when image is loaded */
  .lazy-image.loaded + .shimmer-overlay {
    opacity: 0;
    animation: none;
    transition: opacity 0.4s ease;
  }
  
  /* Image loading states */
  .lazy-image {
    opacity: 0;
    transition: opacity 0.6s cubic-bezier(0.4, 0, 0.2, 1);
    will-change: opacity;
  }
  
  .lazy-image.loading {
    opacity: 0;
  }
  
  .lazy-image.loaded {
    opacity: 1;
  }
  
  /* Optimizations */
  .lazy-image {
    content-visibility: auto;
    contain: layout style paint;
  }
  
  /* Reduce motion for accessibility */
  @media (prefers-reduced-motion: reduce) {
    .shimmer-overlay {
      animation: none;
      background: rgba(96, 165, 250, 0.05);
    }
    
    .lazy-image {
      transition: opacity 0.3s ease;
    }
  }
  
